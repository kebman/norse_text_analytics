	"poem": {
		"title": "Hávamál",
		"verses": [{
				"verse": "I.",
				"strophes": [{
						"strophe": "1.",
						"lines": [
							"Gáttir allar,",
							"áðr gangi fram,",
							"um skoðask skyli,",
							"um skyggnast skyli,",
							"því at óvíst er at vita,",
							"hvar óvinir",
							"sitja á fleti fyrir."
						]
					},
					{
						"strophe": "2.",
						"lines": [
							"Gefendr heilir!",
							"Gestr er inn kominn,",
							"hvar skal sitja sjá?",
							"Mjök er bráðr,",
							"sá er á bröndum skal",
							"síns of freista frama."
						]
					},
					{
						"strophe": "3.",
						"lines": [
							"Elds er þörf,",
							"þeims inn er kominn",
							"ok á kné kalinn;",
							"matar ok váða",
							"er manni þörf,",
							"þeim er hefr um fjall farit."
						]
					},
					{
						"strophe": "4.",
						"lines": [
							"Vatns er þörf,",
							"þeim er til verðar kemr,",
							"þerru ok þjóðlaðar,",
							"góðs of æðis,",
							"ef sér geta mætti,",
							"orðs ok endrþögu."
						]
					},
					{
						"strophe": "5.",
						"lines": [
							"Vits er þörf,",
							"þeim er víða ratar;",
							"dælt er heima hvat;",
							"at augabragði verðr,",
							"sá er ekki kann",
							"ok með snotrum sitr."
						]
					},
					{
						"strophe": "6.",
						"lines": [
							"At hyggjandi sinni",
							"skyli-t maðr hræsinn vera,",
							"heldr gætinn at geði;",
							"þá er horskr ok þögull",
							"kemr heimisgarða til,",
							"sjaldan verðr víti vörum,",
							"því at óbrigðra vin",
							"fær maðr aldregi",
							"en mannvit mikit."
						]
					},
					{
						"strophe": "7.",
						"lines": [
							"Inn vari gestr,",
							"er til verðar kemr,",
							"þunnu hljóði þegir,",
							"eyrum hlýðir,",
							"en augum skoðar;",
							"svá nýsisk fróðra hverr fyrir."
						]
					},
					{
						"strophe": "8.",
						"lines": [
							"Hinn er sæll,",
							"er sér of getr",
							"lof ok líknstafi;",
							"ódælla er við þat,",
							"er maðr eiga skal",
							"annars brjóstum í."
						]
					},
					{
						"strophe": "9.",
						"lines": [
							"Sá er sæll,",
							"er sjalfr of á",
							"lof ok vit, meðan lifir;",
							"því at ill ráð",
							"hefr maðr oft þegit",
							"annars brjóstum ór."
						]
					},
					{
						"strophe": "10.",
						"lines": [
							"Byrði betri",
							"berr-at maðr brautu at",
							"en sé mannvit mikit;",
							"auði betra",
							"þykkir þat í ókunnum stað;",
							"slíkt er válaðs vera."
						]
					},
					{
						"strophe": "11.",
						"lines": [
							"Byrði betri",
							"berr-at maðr brautu at",
							"en sé mannvit mikit;",
							"vegnest verra",
							"vegr-a hann velli at",
							"en sé ofdrykkja öls."
						]
					},
					{
						"strophe": "12.",
						"lines": [
							"Er-a svá gótt",
							"sem gótt kveða",
							"öl alda sona,",
							"því at færa veit,",
							"er fleira drekkr",
							"síns til geðs gumi."
						]
					},
					{
						"strophe": "13.",
						"lines": [
							"Óminnishegri heitir",
							"sá er yfir ölðrum þrumir,",
							"hann stelr geði guma;",
							"þess fugls fjöðrum",
							"ek fjötraðr vark",
							"í garði Gunnlaðar."
						]
					},
					{
						"strophe": "14.",
						"lines": [
							"Ölr ek varð,",
							"varð ofrölvi",
							"at ins fróða Fjalars;",
							"því er ölðr bazt,",
							"at aftr of heimtir",
							"hverr sitt geð gumi."
						]
					},
					{
						"strophe": "15.",
						"lines": [
							"Þagalt ok hugalt",
							"skyli þjóðans barn",
							"ok vígdjarft vera;",
							"glaðr ok reifr",
							"skyli gumna hverr,",
							"unz sinn bíðr bana."
						]
					},
					{
						"strophe": "16.",
						"lines": [
							"Ósnjallr maðr",
							"hyggsk munu ey lifa,",
							"ef hann við víg varask;",
							"en elli gefr",
							"hánum engi frið,",
							"þótt hánum geirar gefi."
						]
					},
					{
						"strophe": "17.",
						"lines": [
							"Kópir afglapi",
							"er til kynnis kemr,",
							"þylsk hann um eða þrumir;",
							"allt er senn,",
							"ef hann sylg of getr,",
							"uppi er þá geð guma."
						]
					},
					{
						"strophe": "18.",
						"lines": [
							"Sá einn veit",
							"er víða ratar",
							"ok hefr fjölð of farit,",
							"hverju geði",
							"stýrir gumna hverr,",
							"sá er vitandi er vits."
						]
					},
					{
						"strophe": "19.",
						"lines": [
							"Haldi-t maðr á keri,",
							"drekki þó at hófi mjöð,",
							"mæli þarft eða þegi,",
							"ókynnis þess",
							"vár þik engi maðr,",
							"at þú gangir snemma at sofa."
						]
					},
					{
						"strophe": "20.",
						"lines": [
							"Gráðugr halr,",
							"nema geðs viti,",
							"etr sér aldrtrega;",
							"oft fær hlægis,",
							"er með horskum kemr,",
							"manni heimskum magi."
						]
					},
					{
						"strophe": "21.",
						"lines": [
							"Hjarðir þat vitu,",
							"nær þær heim skulu,",
							"ok ganga þá af grasi;",
							"en ósviðr maðr",
							"kann ævagi",
							"síns of mál maga."
						]
					},
					{
						"strophe": "22.",
						"lines": [
							"Vesall maðr",
							"ok illa skapi",
							"hlær at hvívetna;",
							"hittki hann veit,",
							"er hann vita þyrfti,",
							"at hann er-a vamma vanr."
						]
					},
					{
						"strophe": "23.",
						"lines": [
							"Ósviðr maðr",
							"vakir um allar nætr",
							"ok hyggr at hvívetna;",
							"þá er móðr,",
							"er at morgni kemr,",
							"allt er víl sem var."
						]
					},
					{
						"strophe": "24.",
						"lines": [
							"Ósnotr maðr",
							"hyggr sér alla vera",
							"viðhlæjendr vini;",
							"hittki hann fiðr,",
							"þótt þeir um hann fár lesi,",
							"ef hann með snotrum sitr."
						]
					},
					{
						"strophe": "25.",
						"lines": [
							"Ósnotr maðr",
							"hyggr sér alla vera",
							"viðhlæjendr vini;",
							"þá þat finnr,",
							"er at þingi kemr,",
							"at hann á formælendr fáa."
						]
					},
					{
						"strophe": "26.",
						"lines": [
							"Ósnotr maðr",
							"þykkisk allt vita,",
							"ef hann á sér í vá veru;",
							"hittki hann veit,",
							"hvat hann skal við kveða,",
							"ef hans freista firar."
						]
					},
					{
						"strophe": "27.",
						"lines": [
							"Ósnotr maðr,",
							"er með aldir kemr,",
							"þat er bazt, at hann þegi;",
							"engi þat veit,",
							"at hann ekki kann,",
							"nema hann mæli til margt;",
							"veit-a maðr,",
							"hinn er vettki veit,",
							"þótt hann mæli til margt."
						]
					},
					{
						"strophe": "28.",
						"lines": [
							"Fróðr sá þykkisk,",
							"er fregna kann",
							"ok segja it sama;",
							"eyvitu leyna",
							"megu ýta synir,",
							"því er gengr um guma."
						]
					},
					{
						"strophe": "29.",
						"lines": [
							"Ærna mælir,",
							"sá er æva þegir,",
							"staðlausu stafi;",
							"hraðmælt tunga,",
							"nema haldendr eigi,",
							"oft sér ógótt of gelr."
						]
					},
					{
						"strophe": "30.",
						"lines": [
							"At augabragði",
							"skal-a maðr annan hafa,",
							"þótt til kynnis komi;",
							"margr þá fróðr þykkisk,",
							"ef hann freginn er-at",
							"ok nái hann þurrfjallr þruma."
						]
					},
					{
						"strophe": "31.",
						"lines": [
							"Fróðr þykkisk,",
							"sá er flótta tekr,",
							"gestr at gest hæðinn;",
							"veit-a görla,",
							"sá er of verði glissir,",
							"þótt hann með grömum glami."
						]
					},
					{
						"strophe": "32.",
						"lines": [
							"Gumnar margir",
							"erusk gagnhollir,",
							"en at virði vrekask;",
							"aldar róg",
							"þat mun æ vera,",
							"órir gestr við gest."
						]
					},
					{
						"strophe": "33.",
						"lines": [
							"Árliga verðar",
							"skyli maðr oft fáa,",
							"nema til kynnis komi:",
							"str ok snópir,",
							"lætr sem solginn sé",
							"ok kann fregna at fáu."
						]
					},
					{
						"strophe": "34.",
						"lines": [
							"Afhvarf mikit",
							"er til ills vinar,",
							"þótt á brautu búi,",
							"en til góðs vinar",
							"liggja gagnvegir,",
							"þótt hann sé firr farinn."
						]
					},
					{
						"strophe": "35.",
						"lines": [
							"Ganga skal,",
							"skal-a gestr vera",
							"ey í einum stað;",
							"ljúfr verðr leiðr,",
							"ef lengi sitr",
							"annars fletjum á."
						]
					},
					{
						"strophe": "36.",
						"lines": [
							"Bú er betra,",
							"þótt lítit sé,",
							"halr er heima hverr;",
							"þótt tvær geitr eigi",
							"ok taugreftan sal,",
							"þat er þó betra en bæn."
						]
					},
					{
						"strophe": "37.",
						"lines": [
							"Bú er betra,",
							"þótt lítit sé,",
							"halr er heima hverr;",
							"blóðugt er hjarta,",
							"þeim er biðja skal",
							"sér í mál hvert matar."
						]
					},
					{
						"strophe": "38.",
						"lines": [
							"Vápnum sínum",
							"skal-a maðr velli á",
							"feti ganga framar,",
							"því at óvíst er at vita,",
							"nær verðr á vegum úti",
							"geirs of þörf guma."
						]
					},
					{
						"strophe": "39.",
						"lines": [
							"Fannk-a ek mildan mann",
							"eða svá matar góðan,",
							"at væri-t þiggja þegit,",
							"eða síns féar",
							"svági [glöggvan],",
							"at leið sé laun, ef þægi."
						]
					},
					{
						"strophe": "40.",
						"lines": [
							"Féar síns,",
							"er fengit hefr,",
							"skyli-t maðr þörf þola;",
							"oft sparir leiðum,",
							"þats hefr ljúfum hugat;",
							"margt gengr verr en varir."
						]
					},
					{
						"strophe": "41.",
						"lines": [
							"Vápnum ok váðum",
							"skulu vinir gleðjask;",
							"þat er á sjalfum sýnst;",
							"viðrgefendr ok endrgefendr",
							"erusk lengst vinir,",
							"ef þat bíðr at verða vel."
						]
					},
					{
						"strophe": "42.",
						"lines": [
							"Vin sínum",
							"skal maðr vinr vera",
							"ok gjalda gjöf við gjöf;",
							"hlátr við hlátri",
							"skyli hölðar taka",
							"en lausung við lygi."
						]
					},
					{
						"strophe": "43.",
						"lines": [
							"Vin sínum",
							"skal maðr vinr vera,",
							"þeim ok þess vin;",
							"en óvinar síns",
							"skyli engi maðr",
							"vinar vinr vera."
						]
					},
					{
						"strophe": "44.",
						"lines": [
							"Veiztu, ef þú vin átt,",
							"þann er þú vel trúir,",
							"ok vill þú af hánum gótt geta,",
							"geði skaltu við þann blanda",
							"ok gjöfum skipta,",
							"fara at finna oft."
						]
					},
					{
						"strophe": "45.",
						"lines": [
							"Ef þú átt annan,",
							"þanns þú illa trúir,",
							"vildu af hánum þó gótt geta,",
							"fagrt skaltu við þann mæla",
							"en flátt hyggja",
							"ok gjalda lausung við lygi."
						]
					},
					{
						"strophe": "46.",
						"lines": [
							"Það er enn of þann",
							"er þú illa trúir",
							"ok þér er grunr at hans geði,",
							"hlæja skaltu við þeim",
							"ok um hug mæla;",
							"glík skulu gjöld gjöfum."
						]
					},
					{
						"strophe": "47.",
						"lines": [
							"Ungr var ek forðum,",
							"fór ek einn saman,",
							"þá varð ek villr vega;",
							"auðigr þóttumk,",
							"er ek annan fann,",
							"maðr er manns gaman."
						]
					},
					{
						"strophe": "48.",
						"lines": [
							"Mildir, fræknir",
							"menn bazt lifa,",
							"sjaldan sút ala;",
							"en ósnjallr maðr",
							"uggir hotvetna,",
							"sýtir æ glöggr við gjöfum."
						]
					},
					{
						"strophe": "49.",
						"lines": [
							"Váðir mínar",
							"gaf ek velli at",
							"tveim trémönnum;",
							"rekkar þat þóttusk,",
							"er þeir rift höfðu;",
							"neiss er nökkviðr halr."
						]
					},
					{
						"strophe": "50.",
						"lines": [
							"Hrörnar þöll,",
							"sú er stendr þorpi á,",
							"hlýr-at henni börkr né barr;",
							"svá er maðr,",
							"sá er manngi ann.",
							"Hvat skal hann lengi lifa?"
						]
					},
					{
						"strophe": "51.",
						"lines": [
							"Eldi heitari",
							"brennr með illum vinum",
							"friðr fimm daga,",
							"en þá sloknar,",
							"er inn sétti kemr,",
							"ok versnar allr vinskapr."
						]
					},
					{
						"strophe": "52.",
						"lines": [
							"Mikit eitt",
							"skal-a manni gefa;",
							"oft kaupir sér í litlu lof,",
							"með halfum hleif",
							"ok með höllu keri",
							"fekk ek mér félaga."
						]
					},
					{
						"strophe": "53.",
						"lines": [
							"Lítilla sanda",
							"lítilla sæva",
							"lítil eru geð guma;",
							"því allir menn",
							"urðu-t jafnspakir;",
							"half er öld hvar."
						]
					},
					{
						"strophe": "54.",
						"lines": [
							"Meðalsnotr",
							"skyli manna hverr;",
							"æva til snotr sé;",
							"þeim er fyrða",
							"fegrst at lifa,",
							"er vel margt vitu."
						]
					},
					{
						"strophe": "55.",
						"lines": [
							"Meðalsnotr",
							"skyli manna hverr,",
							"æva til snotr sé;",
							"því at snotrs manns hjarta",
							"verðr sjaldan glatt,",
							"ef sá er alsnotr, er á."
						]
					},
					{
						"strophe": "56.",
						"lines": [
							"Meðalsnotr",
							"skyli manna hverr,",
							"æva til snotr sé;",
							"örlög sín",
							"viti engi fyrir,",
							"þeim er sorgalausastr sefi."
						]
					},
					{
						"strophe": "57.",
						"lines": [
							"Brandr af brandi",
							"brenn, unz brunninn er,",
							"funi kveikisk af funa;",
							"maðr af manni",
							"verðr at máli kuðr,",
							"en til dælskr af dul."
						]
					},
					{
						"strophe": "58.",
						"lines": [
							"Ár skal rísa,",
							"sá er annars vill",
							"fé eða fjör hafa;",
							"sjaldan liggjandi ulfr",
							"lær of getr",
							"né sofandi maðr sigr."
						]
					},
					{
						"strophe": "59.",
						"lines": [
							"Ár skal rísa,",
							"sá er á yrkjendr fáa,",
							"ok ganga síns verka á vit;",
							"margt of dvelr,",
							"þann er um morgin sefr,",
							"hálfr er auðr und hvötum."
						]
					},
					{
						"strophe": "60.",
						"lines": [
							"Þurra skíða",
							"ok þakinna næfra,",
							"þess kann maðr mjöt,",
							"þess viðar,",
							"er vinnask megi",
							"mál ok misseri."
						]
					},
					{
						"strophe": "61.",
						"lines": [
							"Þveginn ok mettr",
							"ríði maðr þingi at,",
							"þótt hann sé-t væddr til vel;",
							"skúa ok bróka",
							"skammisk engi maðr",
							"né hests in heldr,",
							"þótt hann hafi-t góðan"
						]
					},
					{
						"strophe": "62.",
						"lines": [
							"Snapir ok gnapir,",
							"er til sævar kemr,",
							"örn á aldinn mar;",
							"svá er maðr,",
							"er með mörgum kemr",
							"ok á formælendr fáa."
						]
					},
					{
						"strophe": "63.",
						"lines": [
							"Fregna ok segja",
							"skal fróðra hverr,",
							"sá er vill heitinn horskr;",
							"einn vita",
							"né annarr skal,",
							"þjóð veit, ef þrír ro."
						]
					},
					{
						"strophe": "64.",
						"lines": [
							"Ríki sitt",
							"skyli ráðsnotra",
							"hverr í hófi hafa;",
